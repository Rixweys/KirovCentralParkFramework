<Window x:Class="KirovCentralParkFramework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KirovCentralParkFramework"
        mc:Ignorable="d"
        Title="KirovCentralPark" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition Height="5*"/>
        </Grid.RowDefinitions>
        <Image Grid.Row="0" Source="/Resources/logo.png" HorizontalAlignment="Left"/>
        <Label Content="KirovCentralPark" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <Frame Name="MainFrame" NavigationUIVisibility="Hidden" Grid.Row="1" Source="/Pages/LoginPage.xaml"/>
    </Grid>
</Window>
